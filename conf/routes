# Routes
GET     /mobimeo/vehicle/:xCoordinate/:yCoordinate/:time      controllers.MobimeoController.getVehicleByCoordinatesAndTime(xCoordinate: Int, yCoordinate: Int, time: String)
GET     /mobimeo/vehicle/:stopId                              controllers.MobimeoController.getVehicleByStopId(stopId: Int)
GET     /mobimeo/delay/:lineId                                controllers.MobimeoController.getDelayForLineId(lineId: Int)
